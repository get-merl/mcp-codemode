{
  "name": "mcp-toolbox",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "bin": {
    "mcp-toolbox": "./dist/bin.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./runtime": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/bin.ts src/index.ts --format esm --target node20 --platform node --dts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:e2e": "vitest run tests/e2e",
    "test:e2e:watch": "vitest watch tests/e2e"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "commander": "^14.0.2",
    "jiti": "^2.6.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.8",
    "@vitest/ui": "^2.1.8",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}

