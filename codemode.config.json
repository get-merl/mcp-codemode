{
  "servers": [
    {
      "name": "cloudflare-observability",
      "transport": {
        "type": "stdio",
        "command": "npx",
        "args": ["mcp-remote", "https://observability.mcp.cloudflare.com/mcp"]
      }
    },
    {
      "name": "supabase",
      "transport": {
        "type": "http",
        "url": "https://mcp.supabase.com/mcp?project_ref=grbydozdxbgurgdpkqvv&read_only=true",
        "auth": {
          "type": "bearer",
          "token": "${SUPABASE_ACCESS_TOKEN}"
        }
      }
    }
  ],
  "generation": {
    "outDir": "./codemode",
    "language": "ts"
  },
  "security": {
    "allowStdioExec": true,
    "envAllowlist": ["PATH"]
  },
  "cli": {
    "interactive": true
  }
}
